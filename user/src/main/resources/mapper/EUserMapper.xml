<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ecandy.user.mapper.EUserMapper">
    <select id="selectEUserByName" resultType="com.ecandy.user.model.EUser" parameterType="string">
        select * from e_user where name = #{name}
    </select>
</mapper>
